    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "serverless": "^1.32.0",
    "serverless-dynamodb-local": "^0.2.35",
    "serverless-offline": "^3.27.0",
    "standard": "^12.0.1",
    "supertest": "^3.3.0",
    "chai": "^4.2.0"
  },
  "dependencies": {
    "aws-sdk": "^2.332.0"
