  "main": "handler.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvsa/cvs-svc-test-types.git"
  },
  "scripts": {
    "start": "npm run eslint && ENV=local ./node_modules/serverless/bin/serverless offline start",
    "debug": "export SLS_DEBUG=* && ENV=local node --inspect ./node_modules/serverless/bin/serverless offline start",
    "test": "node_modules/.bin/mocha tests/**/*.unitTest.js",
    "test-i": "node_modules/.bin/mocha tests/**/*.intTest.js",
    "dynamodb": "sls dynamodb start",
    "prepush": "ENV=local npm test && npm run eslint",
    "predeploy": "npm install && npm run prepush",
    "security-checks": "git secrets --scan && git log -p | scanrepo",
    "eslint": "eslint . --config=.eslintrc.json --fix"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
